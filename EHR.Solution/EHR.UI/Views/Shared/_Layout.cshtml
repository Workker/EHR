<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <section id="super">
        <header class="slim">
            @Html.Partial("_HeadBar")
        </header>
        <div class="page">
            <div id="contentCol" class="fixed_elem">
                <div id="content" class="clearfix">
                    @RenderSection("PageLayout")
                        @RenderBody()
                </div>
            </div>
        </div>
    </section>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/AjaxFlagMenu-1.0.2.min.js"></script>
    <script src="~/Scripts/jquery.tokeninput.js"></script>
    <script src="~/Scripts/exporting.js"></script>
    <script src="~/Scripts/highcharts.js"></script>
    @RenderSection("scripts", false)
    <script src="~/Scripts/EHR-Script.js"></script>
</body>
</html>
