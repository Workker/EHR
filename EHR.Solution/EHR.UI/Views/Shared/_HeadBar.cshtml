<div id="topBar" class="fixed_elem">
    <div id="headContent" class="clearfix">
        <h1 id="pageLogo"><a href="#">Rede D'or</a></h1>
        <div id="notifications" class="lfloat" style="padding-top: 3px;">
            <img src="../../Images/Capture.PNG" alt="notifications" />
        </div>
        <div id="headNav" class="clearfix">
            <form class="lfloat" method="get" id="navSearch" name="navSearch" action="results" role="search">
                <div class="uiTypeahead" id="upjgpe42">
                    <div class="wrap" id="search">
                        <div class="innerWrap searchBox">
                            <span class="uiSearchInput textInput searchContainer" id="upjgpe43">
                                <input type="text" class="inputtext DOMControl_placeholder" maxlength="100" id="searchPeople"  data-bind="value: query, valueUpdate: 'afterkeyup', event: { keyup: doSearch }">
                                <button title="Search for people, places and things" data-bind="click: doSearch">
                                    <span class="accessible_elem">Search for people, places and things</span>
                                </button>
                            </span>
                            <ul class="results" data-bind='template: { name: "resultItem", foreach: results }, visible: results().length > 0'>
                                <li class="calltoaction">
                                    <a href="../Search/" rel="ignore">
                                        <span class="text">
                                            <span class="seeMore">See more results for c
                                                <span class="arrow"></span>
                                            </span>
                                            <span class="subtext">Displaying top 3 results</span>
                                        </span>
                                    </a>
                                </li>
                            </ul>
                            <script type="text/html" id="resultItem">
                                <li class="${ type() }">{{if type() == "header"}}
                                    <span data-bind="text: text"></span>
                                    {{else}}
                                    <a href="#">
                                        <img src="${ imageUrl() }" />
                                        <span class="text" data-bind="text: name"></span>
                                        <span class="subtext">bla</span>
                                    </a>
                                    {{/if}}
                                </li>
                            </script>

                        </div>
                    </div>
                    <div class="uiTypeaheadView wideSearchTypeaheadView mainSearchTypeaheadView" id="upjgpe41"></div>
                </div>
            </form>
            <div class="rfloat">
                <ul id="pageNav" class="clearfix">
                    <li class="navItem firstItem tinyman">
                        <img src="../../Images/user.png" alt="user" /><a href="#">User</a></li>
                    <li class="navItem middleItem voiceBarToggle"></li>
                    <li class="navItem middleItem" id="navHome"><a href="#">Home</a></li>
                    <li id="navAccount"><a href="#" id="navAccountLink">
                        <div class="menuPulldown" id="userNavigationLabel">Account Settings</div>
                    </a>
                        <ul class="navigation">
                            <li></li>
                            <li></li>
                            <li></li>
                            <li></li>
                            <li class="menuDivider"></li>
                            <li></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
