@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Page
{
    <section id="super">
        <header class="slim">
            @Html.Partial("_HeadBar")
        </header>
        <div class="page">
            <div id="contentCol" class="fixed_elem">
                <div id="content" class="clearfix">
                    @RenderSection("PageLayout")
                    @RenderBody()
                </div>
            </div>
        </div>
    </section>
}

@section PageLayout
                    {
    <div class="header">
        @{Html.RenderPartial("Layout/_ProfileHeader");}
    </div>
    <aside id="timeLine" class="lfloat">
        @{Html.RenderPartial("Layout/_TimeLine");}
    </aside>
    <div class="verticalLine"></div>
    <aside class="lfloat Menu">
        @{Html.RenderPartial("Layout/_Data");}
    </aside>
}
<div id="center" class="lfloat contentPage">
    @{Html.RenderPartial("_GeneralData");}
    <div id="test"></div>
</div>
@section scripts
{
    <script type="text/javascript" src="~/Scripts/AjaxFlagMenu-1.0.2.min.js"></script>
    <script type="text/javascript" src="~/Scripts/highslide-with-gallery.js"></script>
}