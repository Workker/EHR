<form id="medicationForm">
    <input type="hidden" name="medicationType" value="" />
    <label>Medicamento:</label>
    <input type="text" name="def" id="def" required value="" />
    <br />
    <br />
    <label>Apresentação:</label>
    <input type="text" name="presentation" required value="" />
    <select name="presentationType" required>
        <option value="-1">Selecione</option>
    </select>
    <br />
    <br />
    <label>Dose:</label>
    <input type="text" name="dose" required value="" />
    <select name="dosagem" required>
        <option value="-1">Selecione</option>
    </select>
    <br />
    <br />
    <label>Via:</label>
    <select name="via" required>
        <option value="-1">Selecione</option>
    </select>
    <label>Local:</label>
    <input type="text" name="local" required value="" />
    <br />
    <br />
    <label>Frequência:</label>
    <select name="frequency" required>
        <option value="-1">Selecione</option>
    </select>
    <select name="frequencyCase" required>
        <option value="-1">Selecione</option>
    </select><br />
    <br />
    <label>Duração:</label>
    <input type="number" name="duration" value="" />&nbsp;&nbsp;Dia(s)<br />
    <br />
    <br />
    <input type="submit" onclick='saveRow(this, "../Patient/SaveMedicamention");' value="Savar" class="uibutton large confirm" />&nbsp;&nbsp;
    <input type="reset" onclick="closeForm(this);" value="Cancelar" class="uibutton large" />
</form>
