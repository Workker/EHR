@using EHR.UI.Models
<div class="rfloat width50P leftLine">
    <div style="padding-left: 10px;">
        <h1 class="bottomLine">Solicita&ccedil;&otilde;es de Aprova&ccedil;&atilde;o</h1>
        <br />
        <div id="DivResult">
            @{
                Html.RenderPartial("_AccountApprovedListResult");
            }
        </div>
        @if (((List<AccountModel>)ViewBag.Accounts).Count > 10)
        { 
            <div class="clearfix uiMorePager uiMorePagerCenter showMore" id="moreAccounts">
